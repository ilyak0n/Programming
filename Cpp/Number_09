#include <iostream>
using namespace std;
int main() {
    setlocale(LC_ALL, "Russian");
    cout << "Введите год: ";
    int a;
    cin >> a;
    if ((a%4 == 0 and a%100 != 0) or a%400 == 0) {
        cout << "Високосный" << endl;
    }
    else {
        cout << "Не високосный" << endl;
    }
    return 0;
}
