#include <iostream>
using namespace std;
int main() {
    setlocale(LC_ALL, "Russian");
    cout << "Введите год: ";
    int a;
    cin >> a;
    if (a%4 == 0)
    {
        if (a%100 and a%400)
        {
            cout << "Високосный" << endl;
        }
        else
        {
            cout << "Не високосный" << endl;
        }
    }
    else
    {
        cout << "Не високосный" << endl;
    }
    return 0;
}